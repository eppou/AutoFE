data/raw/*
data/processed/*
data/outputs/*
!data/raw/.gitkeep
!data/processed/.gitkeep
!data/outputs/.gitkeep

# Ignorar configs da IDE
.vscode/
.idea/

# Ignorar logs e arquivos temporários
*.log
.DS_Store